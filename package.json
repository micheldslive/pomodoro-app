    "@vitest/ui": "^0.24.3",
    "css-loader": "^6.7.2",
    "dotenv": "^16.0.3",
    "jsdom": "^20.0.1",
    "path": "^0.12.7",
    "postcss": "^8",
    "sass-loader": "^13.2.0",
    "storybook-addon-sass-postcss": "^0.1.3",
    "storybook-react-context": "^0.6.0",
    "style-loader": "^3.3.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
